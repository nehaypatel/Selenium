package Two;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.interactions.Actions;

public class DynamicDropdown {

	public static void main(String[] args) {
		System.setProperty("webdriver.chrome.driver", "E:\\Data\\Cloud\\OneDrive - hk sar baomin inc\\Selenium\\Drivers\\chromedriver.exe");
		WebDriver a = new ChromeDriver();
		a.get("https://www.spicejet.com");
		WebElement Dest= a.findElement(By.xpath(".//*[@id='ctl00_mainContent_ddl_originStation1_CTXT']"));
		Actions action = new Actions(a);

		action.moveToElement(Dest).click().perform();

		//a.findElement(By.cssSelector("a[value='DEL']")).click(); 

		//a.findElement(By.xpath("//a[@value='IXJ']")).click();
		//a.findElement(By.id("ctl00_mainContent_ddl_originStation1")).click();
//a.findElement(By.xpath("//a[@value='AMD']"));
}

}
